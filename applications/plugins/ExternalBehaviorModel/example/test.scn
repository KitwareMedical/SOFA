<?xml version="1.0"?>

<Node name="Root" gravity="0 0 0" time="0" animate="0" dt="0.02" >
  
      <RequiredPlugin pluginName="ExternalBehaviorModel"/>
	
	
      <VisualStyle displayFlags="hideVisualModels showBehaviorModels showForceFields showCollisionModels" />


      <EulerImplicitSolver verbose="0" rayleighMass="0" rayleighStiffness="0" vdamping="0" />
      <CGSolver verbose="0" iterations="100" tolerance="1.0e-10" threshold="1.0e-10" />


	<Node name="external behavior model" >
	  
	    <MechanicalObject name="exposedDOF" template="Vec3d" showObject="1" showObjectScale="10"/>
	    <RegularGrid />	    
	    <FixedConstraint indices="0 1 2 3" />
	  	    
	    <FEMGridBehaviorModel youngModulus="100" density="0.1" subdivisions="4"/>
	
	</Node>
	
</Node>
 
