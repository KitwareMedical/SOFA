<?xml version="1.0"?>
<Node 	name="Root" gravity="0 0 0 " dt="1"  >
    <VisualStyle displayFlags="showWireframe  " />
    <DefaultAnimationLoop />
    <DefaultVisualManagerLoop />

  <MeshObjLoader name="mesh" filename="mesh/beam.obj" triangulate="1"/>
  <OglModel template="ExtVec3f" name="Visual" fileMesh="mesh/beam.obj" color="0.5 0.8 0.5 0.6"/>

            <MeshToImageEngine template="ImageUC" name="rasterizer" src="@mesh" voxelSize="0.02" padSize="1" rotateImage="false" />
	    <ImageContainer template="ImageUC" name="image" src="@rasterizer" drawBB="false"/>
	    <ImageViewer template="ImageUC" src="@image" />
	    <ImageExporter template="ImageUC" src="@image" filename="/home/benjamin/Sofa/applications-dev/plugins/FlexibleDev/examples/mesh/beam.raw"/>

</Node>
